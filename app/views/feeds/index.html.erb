<h1>Listing all Feeds </h1>

<p>
 <%= link_to 'create new Feed' , new_feed_path%>
</p>
<table>
	<tr>
		<th>Title</th>
		<th>Discription</th>
	</tr>

	<% @feeds.each do |feed| %>
	<tr>
		<td><%= feed.post%></td>
		<td><%= feed.id%></td>
		<td><%= link_to "show" , feed_path(feed)%></td>
		<td><%=link_to "edit" , edit_feed_path(feed)%>
		<td><%=link_to "delete" , feed_path(feed), method: :delete , data: {confirm: "Are you sure"}%>


	</tr>
	<%end%>
</table>